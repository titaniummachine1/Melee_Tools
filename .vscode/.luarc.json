{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "Lua.runtime.version": "Lua 5.4",
    "Lua.runtime.path": [
        "?.lua",
        "?/init.lua",
        "?/?.lua"
    ],
    "Lua.workspace.library": [
        "${3rd}/lfs/library",
        "${workspace}/types"
    ],
    "Lua.workspace.checkThirdParty": false,
    "Lua.diagnostics.globals": [
        "client",
        "draw",
        "engine",
        "entities",
        "globals",
        "gui",
        "input",
        "render",
        "warp",
        "filesystem",
        "callbacks",
        "Lua__fileName",
        "printc",
        "KEY_NONE",
        "KEY_LSHIFT",
        "KEY_RSHIFT"
    ],
    "Lua.diagnostics.disable": [
        "unused-local",
        "unused-function"
    ],
    "Lua.completion.enable": true,
    "Lua.completion.callSnippet": "Both",
    "Lua.completion.keywordSnippet": "Both",
    "Lua.hint.enable": true,
    "Lua.hint.paramName": "All",
    "Lua.hint.setType": true,
    "Lua.hint.arrayIndex": "Enable",
    "Lua.telemetry.enable": false,
    "Lua.diagnostics": {
        "globals": [
            "AbsoluteFrameTime",
            "CreateMove",
            "CurTime",
            "DispatchUserMessage",
            "DoPostScreenSpaceEffects",
            "Draw",
            "DrawModel",
            "DrawStaticProps",
            "FireGameEvent",
            "FrameCount",
            "FrameStageNotify",
            "FrameTime",
            "GCRetrieveMessage",
            "GCSendMessage",
            "GetScriptName",
            "KEY_LSHIFT",
            "KEY_NONE",
            "KEY_RSHIFT",
            "LoadScript",
            "Lua__fileName",
            "MaxClients",
            "OnFakeUncrate",
            "OnLobbyUpdated",
            "PostRenderView",
            "ProcessTempEntities",
            "RealTime",
            "RenderView",
            "RenderViewModel",
            "SendNetMsg",
            "SendStringCmd",
            "ServerCmdKeyValues",
            "SetRichPresence",
            "TickCount",
            "TickInterval",
            "Unload",
            "UnloadScript",
            "callbacks",
            "client",
            "draw",
            "engine",
            "entities",
            "filesystem",
            "globals",
            "gui",
            "input",
            "print",
            "printc",
            "render",
            "warp"
        ]
    }
}